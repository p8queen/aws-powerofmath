{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "PolicyLambdaDynamodb",
			"Effect": "Allow",
			"Action": [
				"dynamodb:PutItem",
				"dynamodb:DeleteItem",
				"dynamodb:Query",
				"dynamodb:Scan",
				"dynamodb:GetItem",
				"dynamodb:UpdateItem"
			],
			"Resource": [
				"arn:aws:dyna .... table/PowerOfMathDatabase"
			]
		}
	]
}